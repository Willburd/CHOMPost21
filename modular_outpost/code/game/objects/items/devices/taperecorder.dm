/obj/item/rectape/anna_lore
	name = "old tape"

/obj/item/rectape/proc/add_custom_entry(min,sec,log)
	timestamp += (min * 60) + sec
	var/MM = min
	if(length(MM) == 1) MM = "0[min]"
	var/SS = sec
	if(length(SS) == 1) SS = "0[SS]"
	storedinfo += "\[[MM]:[SS]\] [log]"

